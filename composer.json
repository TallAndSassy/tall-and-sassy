{
    "name": "laravel-frontend-presets/tall-and-sassy-preset",
    "description": "Tall and Sassy preset for Laravel.",
    "keywords": [
        "alpine",
        "alpinejs",
        "tailwind",
        "tailwindcss",
        "tailwindui",
        "tall",
        "laravel",
        "livewire",
        "preset"
    ],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/laravel-frontend-presets/tall/issues",
        "source": "https://github.com/laravel-frontend-presets/tall"
    },
    "authors": [
        {
            "name": "Dan Harrin",
            "email": "Dan@DanHarrin.com"
        },
        {
            "name": "Liam Hammett",
            "email": "liam@liamhammett.com"
        },
        {
            "name": "Ryan Chandler",
            "email": "support@ryangjchandler.co.uk"
        }
    ],
    "require": {
        "php": "^7.4.8",
        "laravel/ui": "^3.0",
        "illuminate/support": "^8.0",
        "mediconesystems/livewire-datatables": "master-dev",
        "tallandsassy/app-branding": "master-dev",
        "tallandsassy/app-theme-base-admin": "master-dev",
        "tallandsassy/grok-livewire-jet": "master-dev",
        "tallandsassy/team-means-family": "master-dev",
        "tallandsassy/app-theme-base-ui-glances": "master-dev",
        "eleganttechnologies/grok": "master-dev",
        "tallandsassy/grok-jet-ui": "master-dev",
        "tallandsassy/livewire-friends": "master-dev",
        "tallandsassy/plugin-grok": "master-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "LaravelFrontendPresets\\Tall\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelFrontendPresets\\Tall\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelFrontendPresets\\Tall\\TallServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}